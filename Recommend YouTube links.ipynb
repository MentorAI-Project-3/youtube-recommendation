    "def formate_videos_metadata(videos):\n",
    "    metadata = \"\"\n",
    "    \n",
    "    for idx, vid in enumerate(videos, start = 1):\n",
    "        metadata += f\"{idx}. Title: {vid['title']}\\nDescription: {vid['description']}\\n Link: {vid['url']}\\n\\n\"\n",
    "    \n",
    "    return metadata"
   ]
